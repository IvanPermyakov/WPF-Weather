<Window x:Class="Interface_Weather.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Interface_Weather"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="#FFE0FFFC">
    <StackPanel>
        <TextBlock x:Name="Header" HorizontalAlignment="Center" Margin="0,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="216" FontSize="20" TextAlignment="Center"><Run Language="ru-ru" Text="Погода в Перми"/></TextBlock>
        <Grid x:Name="Grid1" Margin="0,74,0,0">
            <Grid.ColumnDefinitions></Grid.ColumnDefinitions>
            <Grid.RowDefinitions></Grid.RowDefinitions>
        </Grid>
    </StackPanel>
</Window>
